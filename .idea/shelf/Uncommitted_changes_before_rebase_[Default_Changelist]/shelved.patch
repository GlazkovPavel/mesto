--.popup__preview-subtitle{color:#fff;font-family:Inter,Arial,Helvetica,"sans-serif";font-size:12px;font-style:normal;font-weight:400;line-height:15px}
--.element{margin:0 auto;max-width:880px}
--.element__grid{-moz-column-gap:17px;column-gap:17px;display:grid;grid-template-columns:repeat(3,1fr);grid-template-rows:repeat(2,1fr);margin:50px auto 0;padding:0;row-gap:20px}@media screen and (max-width:879px){.element__grid{grid-template-columns:repeat(2,1fr);grid-template-rows:repeat(3,1fr);margin:40px 24px 0}}@media screen and (max-width:620px){.element__grid{margin:30px 16px 0}}@media screen and (max-width:520px){.element__grid{grid-template-columns:repeat(1,1fr);grid-template-rows:repeat(6,1fr);margin:40px 19px 0}}
--.element__foto{border-top-left-radius:10px;border-top-right-radius:10px;cursor:pointer;height:282px;margin:0;-o-object-fit:cover;object-fit:cover;width:100%}
--.element__description{display:flex;justify-content:space-between}
--.element__description-text{color:#000;font-family:Inter,Arial,Helvetica,"sans-serif";font-size:24px;font-style:normal;font-weight:900;line-height:29px;margin:0;overflow:hidden;padding-bottom:31px;padding-left:21px;padding-top:25px;text-overflow:ellipsis;white-space:nowrap}
--.element__description-like{align-self:center;background:url(5126b6ad6039c01c21cb.svg);background-repeat:no-repeat;border:none;cursor:pointer;height:19px;justify-self:self-end;margin-right:20px;outline:none;padding-right:14px;width:22px}.element__description-like:hover{opacity:50%}
--.element__container{background-color:#fff;border-radius:10px;list-style:none;max-width:282px}@media (max-width:879px){.element__container{border-radius:15px;max-width:100%;overflow:hidden}}
--.element__description-like_active{background:url(fe17a515669479b1e649.svg);background-repeat:no-repeat}
--.element__trash{background:url(cbc09a894bdd4c956365.svg);background-position:50%;background-repeat:no-repeat;border:none;cursor:pointer;height:19.3px;margin-left:-33px;margin-top:18px;outline:none;position:absolute;width:18px;z-index:2}.element__trash:hover{opacity:.6}
--.popup__item_type_error{border-bottom-color:red}
--.popup__input-error_active{color:red;font-size:12px;line-height:15px}
